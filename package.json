{
  "$schema": "http://json.schemastore.org/package",
  "name": "@n4bb12/apollo-cra-example",
  "version": "0.0.0-development",
  "private": true,
  "description": "An example of using GraphQL with apollo-server, appollo-client, and create-react-app",
  "keywords": [
    "apollo-server",
    "apollo-client",
    "create-react-app",
    "graphql",
    "graphql-codegen"
  ],
  "homepage": "https://github.com/n4bb12/apollo-cra-example/blob/master/README.md",
  "bugs": "https://github.com/n4bb12/apollo-cra-example/issues",
  "repository": "github:n4bb12/apollo-cra-example",
  "license": "ISC",
  "author": "Abraham Schilling",
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:client": "react-scripts build",
    "build:server": "tsc --project tsconfig.server.json",
    "codegen": "graphql-codegen",
    "dev": "run-p --print-label dev:*",
    "dev:client": "react-scripts start",
    "dev:server": "nodemon --watch src/server --ext ts --exec ts-node --project tsconfig.server.json src/server/index.ts",
    "fix": "run-p --print-label typecheck format",
    "format": "prettier --ignore-path .gitignore --write .",
    "start": "node dist/server/index.js",
    "test": "react-scripts test",
    "typecheck": "run-p --print-label typecheck:*",
    "typecheck:client": "tsc --noEmit --project tsconfig.client.json",
    "typecheck:server": "tsc --noEmit --project tsconfig.server.json"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "apollo-server": "^3.6.3",
    "apollo-server-core": "^3.6.3",
    "graphql": "^16.3.0",
    "ow": "^0.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "graphql-config": "^4.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-scripts": "5.0.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
